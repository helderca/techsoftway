<?xml version="1.0" encoding="UTF-8"?>

<!DOCTYPE hibernate-mapping PUBLIC "-//Hibernate/Hibernate Mapping DTD 3.0//EN" "http://hibernate.sourceforge.net/hibernate-mapping-3.0.dtd">
<hibernate-mapping>

	<class name="Model.Prontuario">

		<id name="codProntuario" column="codprontuario">
			<generator class="increment">
			</generator>
		</id>

		<property name="descricaoProntuario" column="descricaoprontuario"
			length="10">
		</property>

		<property name="nomePaciente" column="nomepaciente" length="10">
		</property>





	</class>
</hibernate-mapping>